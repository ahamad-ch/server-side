{
  "name": "admin-services",
  "version": "1.0.0",
  "description": "Service which provides all the Admin services for Archi's",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon ./server.js --exec \"npm run lint:fix && node\"",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix",
      "git add"
    ],
    "*.+(json|css|md)": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:academyarchis/admin-services.git"
  },
  "author": "tofi",
  "license": "ISC",
  "bugs": {
    "url": "https://archisresearch.atlassian.net/secure/RapidBoard.jspa"
  },
  "homepage": "https://bitbucket.org/academyarchis/mail-service/src/master/README.md",
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.5"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "i18n": "^0.9.0",
    "nodemailer": "^6.3.1",
    "swagger-jsdoc": "^6.0.1",
    "swagger-ui-express": "^4.1.6"
  }
}
